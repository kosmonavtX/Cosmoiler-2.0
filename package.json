{
  "name": "cosmoiler",
  "private": true,
  "version": "4.9.1",
  "description": "Cosmoiler - automatic chain oiler",
  "repository": "",
  "license": "FREE",
  "homepage": "./",
  "author": "Sergey Borisov <cosmoiler@gmail.com> (https://cosmoiler.com)",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./build/webpack.config.js",
    "buildcpx": "cross-env NODE_ENV=production node ./build/build.js && npm run postinstall",
    "build": "cross-env NODE_ENV=production node ./build/build.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production node ./build/build.js && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production node ./build/build.js && npx cap copy android",
    "postinstall": "cpx ./www/*.{json,html,gz,woff,png} ./%USERPROFILE%/Documents/Prog/Firmware/ESP32/workspace/cosmoiler-idf/data/"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@capacitor/android": "^2.4.6",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "^2.4.6",
    "cpx": "^1.5.0",
    "debug": "^4.3.1",
    "dom7": "^3.0.0",
    "framework7": "^6.0.17",
    "framework7-icons": "^3.0.1",
    "framework7-svelte": "^6.0.17",
    "reconnecting-websocket": "^4.4.0",
    "skeleton-elements": "^3.3.0",
    "svelte": "^3.38.2",
    "svelte-i18n": "^3.3.0",
    "swiper": "^6.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@capacitor/cli": "^2.4.6",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cordova-res": "^0.15.3",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
